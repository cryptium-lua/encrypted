local HttpService = game:GetService("HttpService")

local CustomFont = {}
function CustomFont:New(Name, Weight, Style, Data)
    if not isfile(Data.Id) then 
        writefile(Data.Id, game:HttpGet(Data.Url))
    end

    local fontData = {
        name = Name,
        faces = {
            {
                name = Name,
                weight = Weight,
                style = Style,
                assetId = getcustomasset(Data.Id)
            }
        }
    }

    writefile("fonts/" .. Name .. ".font", HttpService:JSONEncode(fontData))
    return Font.new(getcustomasset("fonts/" .. Name .. ".font"), Enum.FontWeight.Regular, Enum.FontStyle.Normal)
end

