local function CreateTabButton(text)
    local TabButton = Instance.new("TextButton")
    local TabUIStroke = Instance.new("UIStroke")
    local label = Instance.new("TextLabel")

    TabButton.Size = UDim2.new(0.95, 0, 0, 25)
    TabButton.Text = ""
    TabButton.BorderSizePixel = 0
    TabButton.BorderColor3 = Color3.fromRGB(24,23,20)
    TabButton.BackgroundTransparency = 0
    TabButton.BackgroundColor3 = Color3.fromRGB(30, 28, 25)
    TabButton.Parent = Tabs

    TabUIStroke.Parent = TabButton
    TabUIStroke.Color = Color3.fromRGB(12,12,12)
    TabUIStroke.Thickness = 1
    TabUIStroke.Transparency = 0.6

    label.Size = UDim2.new(0.75, 0, 1, 0)
    label.Position = UDim2.new(0, 15, 0, 0)
    label.Text = text
    label.TextColor3 = Color3.fromRGB(235,235,235)
    label.BorderSizePixel = 0
    label.BackgroundTransparency = 1
    label.TextSize = 14
    label.FontFace = Custom.Font
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = TabButton

    return TabButton
end

