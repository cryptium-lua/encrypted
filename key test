Config = {
    api = "9afb4be9-8f64-4039-8b86-f75dd5318eee",
    service = "WL2",
    provider = "WL2"
}

local function main()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/cryptium-lua/encrypted/refs/heads/main/wl2%20haha"))()
    print("Script loaded.")
end

local CollectionService = game:GetService("CollectionService")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local G2L = {}

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "KeySystemUI"
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = game:GetService("CoreGui")
CollectionService:AddTag(ScreenGui, "main")
G2L.ScreenGui = ScreenGui

local function openGetKey()
	local JunkieKeySystem = loadstring(game:HttpGet("https://junkie-development.de/sdk/JunkieKeySystem.lua"))()
	local API_KEY = Config.api
	local PROVIDER = Config.provider
	local SERVICE = Config.service
	local link = JunkieKeySystem.getLink(API_KEY, PROVIDER, SERVICE)
	if link then
		if setclipboard then
			setclipboard(link)
				wait(0.5)
			Player:Kick("Link copied to clipboard.")
		else
print("Link generated.")
		end
	else
print("Failed to generate link.")
	end
end

local function validateKey()
	local userKey = getgenv().Key
	if not userKey or userKey == "" then
print("Please enter a valid key.")
		return
	end
print("Validating Key...")
	local JunkieKeySystem = loadstring(game:HttpGet("https://junkie-development.de/sdk/JunkieKeySystem.lua"))()
	local API_KEY = Config.api
	local SERVICE = Config.service
	local isValid = JunkieKeySystem.verifyKey(API_KEY, userKey, SERVICE)
	if isValid then
		print("Valid Key! Loading...")
		task.wait(0.5)
		ScreenGui:Destroy()
		main()
	else
		print("Invalid Key!")
	end
end

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0.47, 0, 0.64, 0)
Frame.Position = UDim2.new(0.28, 0, 0.12, 0)
Frame.BackgroundColor3 = Color3.fromRGB(21, 21, 21)
Frame.Active = true
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui
Frame.Draggable = true
G2L.Frame = Frame

Instance.new("UICorner", Frame).CornerRadius = UDim.new(0, 10)

local Title = Instance.new("TextLabel")
Title.Text = "üîê Junkie Development Key System"
Title.FontFace = Font.new("rbxasset://fonts/families/GothamSSm.json", Enum.FontWeight.Bold)
Title.TextColor3 = Color3.new(1, 1, 1)
Title.TextSize = 18
Title.BackgroundTransparency = 1
Title.Size = UDim2.new(1, 0, 0.12, 0)
Title.Position = UDim2.new(0, 0, 0.1, 0)
Title.Parent = Frame

local KeyBox = Instance.new("TextBox")
KeyBox.PlaceholderText = "Enter your key..."
KeyBox.FontFace = Font.new("rbxasset://fonts/families/GothamSSm.json")
KeyBox.TextColor3 = Color3.new(1, 1, 1)
KeyBox.TextSize = 14
KeyBox.Text = ""
KeyBox.Size = UDim2.new(0.8, 0, 0.165, 0)
KeyBox.Position = UDim2.new(0.1, 0, 0.35, 0)
KeyBox.BackgroundColor3 = Color3.fromRGB(41, 41, 41)
KeyBox.Parent = Frame
Instance.new("UICorner", KeyBox).CornerRadius = UDim.new(0, 6)
G2L.KeyBox = KeyBox

KeyBox.FocusLost:Connect(function()
	getgenv().Key = KeyBox.Text
end)

local function createButton(text, position, callback)
	local button = Instance.new("TextButton")
	button.Text = text
	button.Size = UDim2.new(0.8, 0, 0.165, 0)
	button.Position = position
	button.FontFace = Font.new("rbxasset://fonts/families/GothamSSm.json", Enum.FontWeight.Bold)
	button.TextSize = 16
	button.TextColor3 = Color3.new(1, 1, 1)
	button.BackgroundColor3 = Color3.fromRGB(51, 101, 255)
	button.Parent = Frame
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
	button.MouseButton1Click:Connect(callback)
	return button
end

local VerifyButton = createButton("‚úîÔ∏è Verify Key", UDim2.new(0.1, 0, 0.55, 0), function()
	validateKey()
end)

local GetKeyButton = createButton("üîë Get Key", UDim2.new(0.1, 0, 0.75, 0), function()
	openGetKey()
end)

G2L.VerifyButton = VerifyButton
G2L.GetKeyButton = GetKeyButton

validateKey()

return G2L.ScreenGui, require
